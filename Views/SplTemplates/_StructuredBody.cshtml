@using MedRecPro.Models;
@using RazorLight.Razor;
@using MedRecPro.Helpers;
@inherits RazorLight.TemplatePage<MedRecPro.Models.StructuredBodyDto>;
@model StructuredBodyDto

@if (Model != null)
{
    <structuredBody>
        @foreach (var component in Model.Sections.GroupBy(s => s.SectionGUID).Select(g => g.First()))
        {
            <component>
                @{
                    await IncludeAsync("_Section", component);
                }
            </component>
        }
    </structuredBody>
}